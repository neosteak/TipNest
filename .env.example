# Deployment Configuration Example
# Copy this file to .env.local and fill with your actual values

# Private key for deployment (NEVER commit the real key!)
PRIVATE_KEY=your_private_key_here

# Network RPC URLs
POLYGON_RPC_URL=https://polygon-mainnet.g.alchemy.com/v2/your_alchemy_key_here
POLYGON_AMOY_RPC_URL=https://polygon-amoy.g.alchemy.com/v2/your_alchemy_key_here

# Contract addresses (fill after deployment)
TIP_TOKEN_ADDRESS=0x0000000000000000000000000000000000000000
STAKING_CONTRACT_ADDRESS=0x0000000000000000000000000000000000000000

# Polygonscan API key for contract verification
POLYGONSCAN_API_KEY=your_polygonscan_api_key_here